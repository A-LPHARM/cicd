pipeline {
	agent any

	stages {

		stage("Git Checkout") {
			steps {
				git branch: 'main', url: 'https://github.com/A-LPHARM/cicd.git'
			}
		}

		stage("docker build") {
			steps{
				sh "Docker build -t henriksin/timeserver ."
			}
		}

		stage("Authenticate") {
			steps{
				sh "echo authenticating..."
			}
		}


		stage("Push to Registry") {
			steps{
				sh "echo pushing..."
			}
		}

		stage("Cleanup") {
			steps{
				sh "echo cleanup..."
			}
		}


	}
}